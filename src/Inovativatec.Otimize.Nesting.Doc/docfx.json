{  
  "build": {
    "content": [      
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "tutorial/**.md",
          "tutorial/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "images/**",
          "favicon.ico"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_enableSearch": "true"
    },
    "dest": "_site",
    "template": [
      "default",
      "templates/otimize"
    ]
  }
}
